<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">
    <!--=======================================================-->
	<PTYPE name="VLAN_ID"
        pattern="(409[0-5]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])"
           help="Number in the range 1-4095"/>
        <!--=======================================================-->
        <PTYPE name="AG_TIME"
        pattern="(409[0-5]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])"
           help="Number in the range 1-4095"/>
	<!--=======================================================-->
	<PTYPE name="IP_ADDR"
	    pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))"
	       help="IP address AAA.BBB.CCC.DDD where each part is in the range 0-255"/>
	<!--=======================================================-->
	<PTYPE name="NETMASK"
	    pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))"
	       help="netmask AAA.BBB.CCC.DDD where each part is in the range 0-255"/>
	<!--=======================================================-->
	<PTYPE name="UINT"
	    pattern="[0-9]+"
	       help="Unsigned integer"/>
	<!--=======================================================-->
	<PTYPE name="CLK_OFF"
	    pattern="^(-?){2}(2[0-4]|1[0-9]|[1-9])"
	       help="Clock offset"/>
	<!--=======================================================-->
	<PTYPE name="CLK_NAME"
	    pattern="([a-zA-Z_]+)"
	       help="Clock set name"/>
	<!--=======================================================-->
	<PTYPE name="STRING"
	    pattern="[^\-]+"
	       help="String"/>
	<!--=======================================================-->
	<PTYPE name="COMM_TYPE"
	    pattern="(ro|rw)"
	       help="Community type"/>
	<!--=======================================================-->
	<PTYPE name="VL_STRING"
	    pattern="[0-9\,]+"
	       help="Vl_String"/>
	<!--=======================================================-->
	<PTYPE name="STRING_ALL"
	    pattern="[^\`]+"
	       help="String"/>
	<!--=======================================================-->
	<PTYPE name="STRING_ALL_10"
	    pattern="([^\`]+)"
	       help="String 10 chars"/>
	<!--=======================================================-->		
	<PTYPE name="BOOL"
	    pattern="true(1) false(0)"
	       help="Boolean choice"/>
	<!--=======================================================-->
	<PTYPE name="MAC_ADDR"
	    pattern="((([0-9]|[a-f]|[A-F]){4}\.){2}([0-9]|[a-f]|[A-F]){4})"
	       help="H.H.H  48 bit mac address"/>
	<!--=======================================================-->
	<PTYPE name="IF_ID"
	    pattern="\0\/(1[0-6]|[1-9])"
	       help="Interface id choice"/>
	<!--=======================================================-->
	<PTYPE name="GB_IF_ID"
	    pattern="\0\/([1-2])"
	       help="Interface id choice"/>
	<!--=======================================================-->

</CLISH_MODULE>
